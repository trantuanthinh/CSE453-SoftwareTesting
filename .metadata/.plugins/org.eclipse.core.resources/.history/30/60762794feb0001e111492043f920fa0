import java.util.Scanner;

public class Triangle {
	 public static void main(String[] args) {
	       

	    }

	    public static boolean isTriangle(double a, double b, double c) {
	        if ((a + b > c) &&
	                (a + c >= b) && 
	                (b + c > a)) {
	            return true;
	        }
	        return false;
	    }


}
