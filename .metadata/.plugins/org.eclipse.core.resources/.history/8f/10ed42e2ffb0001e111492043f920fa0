public class Fibonacci {
	
	public static void main(String[] args) {
		String n ="abd";
//		int n = 5 ;
		System.out.println(String.valueOf(fib(n)));
	}
	
	static public int fib(int n) {
		switch (n) {
			case 0: return 0;
			case 1: return 1;
			default: return (fib(n - 1) + fib(n - 2)) + 1;
		}
	}
}
